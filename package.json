    "url": "https://github.com/mauabe/news-boy/issues"
  },
  "homepage": "https://github.com/mauabe/news-boy#readme",
  "dependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "axios": "^0.18.0",
    "express": "^4.16.4",
    "live-server": "^1.2.1",
    "lodash": ">=4.17.15",
    "parcel-bundler": "^1.10.3",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "request": "^2.88.0"
  },
